<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <!-- Parsing settings -->
        <add key="ClientBuild" value=""/>
        <add key="Filters" value=""/>
        <add key="IgnoreFilters" value=""/>
        <add key="IgnoreByEntryFilters" value=""/> <!-- Format: Type:Id1:Id2:Id3,Type2:Id4:Id5:Id6 -->
        <add key="Threads" value="0"/>
        <!-- DumpFormat:
            0: No dump
            1: Text
            2: .bin
            3: .pkt
            4: Text: Only the headers of each packet
            5: Summary Text: Only the header of first occurrence of each Opcode (match by Opcode name and direction)
        -->
        <add key="DumpFormat" value="1"/>
        <add key="PacketsNum" value="0"/>
        <add key="FilterPacketNumLow" value="0"/>
        <add key="FilterPacketNumHigh" value="0"/>
        <add key="ShowEndPrompt" value="false"/>
        <add key="DebugReads" value="false"/>
        
        <!-- SSH Tunneling settings -->
        <add key="SSHEnabled" value="false" />
        <add key="SSHHost" value="" />
        <add key="SSHUsername" value="" />
        <add key="SSHPassword" value="" />
        <add key="SSHPort" value="22" />
        <add key="SSHLocalPort" value="3307" />

        <!-- Database settings -->
        <add key="DBEnabled" value="false" />
        <add key="Server" value="localhost" />
        <add key="Port" value="3306" />
        <add key="Username" value="root" />
        <add key="Password" value="" />
        <add key="Database" value="WPP" />
        <add key="CharacterSet" value="utf8" />

        <!-- Enable SQL Output, Mask
            GameObjectTemplate = 1,
            GameObjectSpawns   = 2,
            QuestTemplate      = 4,
            QuestPOI           = 8,
            CreatureTemplate   = 16,
            CreatureSpawns     = 32,
            NpcTrainer         = 64,
            NpcVendor          = 128,
            NpcText            = 256,
            Loot               = 512,
            Gossip             = 1024,
            PageText           = 2048,
            StartInformation   = 4096,
            SniffData          = 8192,
            SniffDataOpcodes   = 16384,
        -->
        <add key="SQLOutput" value="0"/>
    </appSettings>
</configuration>
